{
	"mcpServers": {
		"ddg-search": {
			"type": "stdio",
			"command": "uvx",
			"args": ["duckduckgo-mcp-server"]
		},
		"serena": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide-assistant",
				"--project",
				"/home/aryaniyaps/web-projects/hospitaljobsin",
				"--enable-web-dashboard",
				"false"
			],
			"env": {}
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp", "--api-key", "${CONTEXT7_API_KEY}"]
		},
		"next-devtools": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "next-devtools-mcp@latest"]
		},
		"playwright": {
			"type": "stdio",
			"command": "npx",
			"args": ["@playwright/mcp@latest"]
		}
	}
}